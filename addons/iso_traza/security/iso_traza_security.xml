	    </record>
	    
	    <record id="base.menu_base_partner" model="ir.ui.menu">
		    <field name="groups_id" eval="[(3,ref('iso_traza.group_traza'))]"/>
		</record>
		
	    <record id="base.menu_base_partner" model="ir.ui.menu">
		    <field name="groups_id" eval="[(3,ref('base.group_user'))]"/>
